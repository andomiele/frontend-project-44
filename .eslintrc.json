{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "semi": ["error", "never"], 
        "object-curly-spacing": ["error", "always"],
        "array-bracket-spacing": ["error", "never"],
        "linebreak-style": ["error", "unix"],
        "no-trailing-spaces": "error",
        "camelcase": "error"
    }
}
